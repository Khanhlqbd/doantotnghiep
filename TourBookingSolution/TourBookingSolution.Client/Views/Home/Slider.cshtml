@model IEnumerable<TourBookingSolution.Data.ViewModels.BannerActionView>
<div class="slider">

    <ul class="slides">
        @if (Model != null && Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <li>
                    <img src="@item.Avatar" alt="">
                </li>
            }
        }
    </ul>

</div>
